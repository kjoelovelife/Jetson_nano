   "cell_type": "code",
   "execution_count": 24,
   "metadata": {},
   "outputs": [],
   "source": [
    "camera.running = False\n",
    "camera.unobserve(update_image, names='value')\n",
    "camera_link.unlink()  # don't stream to browser (will still run camera)"
   ]
